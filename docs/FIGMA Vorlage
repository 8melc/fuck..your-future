  );
}

function Hero() {
  const { width } = useActiveBreakpoint();
  if (width < 800) {
    return <HeroMobile />;
  }
  if (width < 1280) {
    return <HeroTablet />;
  }
  return <HeroDesktop />;
}

export default Hero;
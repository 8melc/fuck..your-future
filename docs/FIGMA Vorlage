import imgHero from "figma:asset/2500fa512f553793f43f7c10fdff0632a7d9ddcc.png";
import { useActiveBreakpoint } from "figma:react";
type BtnProps = {
  className?: string;
  type?: "Default" | "Hover";
};

/**
 * @figmaAssetKey 71a610551406e06e24565c6a0ed3f5341ee52b00
 */
function Btn({ className, type = "Default" }: BtnProps) {
  if (type === "Hover") {
    return (
      <div className={className} data-name="Type=Hover" style={{ backgroundImage: "linear-gradient(90deg, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0.1) 100%), linear-gradient(90deg, rgb(149, 255, 141) 0%, rgb(149, 255, 141) 100%)" }}>
        <div className="flex flex-row items-center size-full">
          <div className="box-border content-stretch flex gap-[6px] items-center px-[19px] py-[12px] relative size-full">
            <div className="content-stretch flex gap-[10px] items-center relative shrink-0 w-[74px]" data-name="content spacing">
              <div className="flex flex-col font-['Roboto_Mono:Medium',_sans-serif] font-medium justify-center leading-[0] relative shrink-0 text-[16px] text-black text-center text-nowrap tracking-[-0.48px] uppercase">
                <p className="leading-[1.4] whitespace-pre">REGISTER</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    );
  }
  return (
    <div className={className} data-name="Type=Default">
      <div className="flex flex-row items-center size-full">
        <div className="box-border content-stretch flex gap-[6px] items-center px-[19px] py-[12px] relative size-full">
          <div className="content-stretch flex gap-[10px] items-center opacity-60 relative shrink-0" data-name="content spacing">
            <div className="flex flex-col font-['Roboto_Mono:Medium',_sans-serif] font-medium justify-center leading-[0] relative shrink-0 text-[16px] text-black text-center text-nowrap tracking-[-0.48px] uppercase">
              <p className="leading-[1.4] whitespace-pre">register</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  );
}

function RunningDesktop() {
  return (
    <div className="content-stretch flex gap-[10px] items-center justify-center relative shrink-0" data-name="running">
      <div className="flex flex-col font-['Righteous:Regular',_sans-serif] justify-center leading-[0] not-italic relative shrink-0 text-[var(--responsive-font-size,25.975px)] text-white tracking-[-0.7792px] w-[92.745px]">
        <p className="leading-[0.9]">Running for Change</p>
      </div>
    </div>
  );
}

function MenuItemsDesktop() {
  return (
    <div className="basis-0 content-stretch cursor-pointer flex flex-col font-['Righteous:Regular',_sans-serif] gap-[4px] grow items-center justify-center leading-[0] min-h-px min-w-px not-italic relative shrink-0 text-right text-white tracking-[-0.7792px]" data-name="menu items">
      <a className="flex flex-col justify-center relative shrink-0 text-[var(--responsive-font-size,25.975px)] w-full" href="/about">
        <p className="leading-[0.9]">About Us</p>
      </a>
      <a className="flex flex-col justify-center relative shrink-0 text-[0px] w-full" href="/event">
        <p className="[text-underline-position:from-font] decoration-solid font-['Righteous:Regular',_sans-serif] leading-[0.9] not-italic text-[var(--responsive-font-size,25.975px)] tracking-[-0.7792px] underline">Events</p>
      </a>
    </div>
  );
}

function NavDesktop() {
